# Database Configuration
DATABASE_URL=postgresql://username:password@hostname:5432/database_name

# Environment
NODE_ENV=development

# Session Configuration (automatically generated in production)
SESSION_SECRET=your_session_secret_here

# Optional: Database Connection Details (used by Drizzle)
PGHOST=hostname
PGPORT=5432
PGUSER=username
PGPASSWORD=password
PGDATABASE=database_name